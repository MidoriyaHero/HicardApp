<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout
        android:id="@+id/linearPhoneOne"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingStart="11dp"
        android:paddingEnd="11dp"
        android:paddingTop="131dp"
        style="@style/groupStylewhite_A700_01">
        <FrameLayout
            android:id="@+id/frameStackBuiT"
            android:layout_gravity="center_horizontal"
            android:layout_width="368dp"
            android:layout_height="293dp">
            <TextView
                android:id="@+id/txtBuitrungtin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top|center"
                android:gravity="top|center"
                android:text="@string/msg_bui_trung_tin_app"
                style="@style/txtInterbold50"
                tools:textAllCaps="true"/>
            <ImageView
                android:id="@+id/imageImageone"
                android:layout_gravity="bottom|center"
                android:layout_width="169dp"
                android:layout_height="165dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_1"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_1"/>

        </FrameLayout>
        <TextView
            android:gravity="center_horizontal"
            android:id="@+id/txtEnjoyget"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginTop= "13dp"
            android:text="@string/msg_enjoy_getting_watched"
            style="@style/txtInterbold40"
            tools:text="@string/msg_enjoy_getting_watched"/>

    </LinearLayout>
        

</layout>