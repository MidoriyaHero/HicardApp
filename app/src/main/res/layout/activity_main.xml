<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tootlbarToolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="602dp"
        android:gravity="center_horizontal"
        app:contentInsetStart="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/text"
        style="@style/txtIntersemibold18"
        android:layout_width="410dp"
        android:layout_height="0dp"
        android:layout_gravity="start"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="8dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text=" Hospital Information"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearColumnabout"
        android:layout_width="403dp"
        android:layout_height="251dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="146dp"
        android:layout_marginBottom="334dp"
        android:orientation="vertical"
        android:paddingStart="13dp"
        android:paddingTop="26dp"
        android:paddingEnd="13dp"
        android:paddingBottom="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearColumndrmacus"
            style="@style/groupStylewhite_A700_01cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            android:orientation="vertical"
            android:paddingStart="8dp"
            android:paddingTop="7dp"
            android:paddingEnd="8dp"
            android:paddingBottom="7dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="367dp"
                    android:layout_height="174dp"
                    android:layout_marginStart="5dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/text1"
                        style="@style/txtIntersemibold18"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_hospital"
                        android:textSize="24sp"
                        tools:text="@string/lbl_hospital" />

                    <TextView
                        android:id="@+id/name"
                        style="@style/txtIntermedium12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text=""
                        android:textSize="16sp"
                        tools:ignore="TextSizeCheck"
                        tools:text="" />

                    <TextView
                        android:id="@+id/phone"
                        style="@style/txtIntermedium12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text=""
                        android:textSize="16sp"
                        tools:ignore="TextSizeCheck"
                        tools:text="" />

                    <TextView
                        android:id="@+id/address"
                        style="@style/txtIntermedium12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:ellipsize="end"
                        android:singleLine="true"

                        android:textSize="16sp"
                        tools:ignore="TextSizeCheck" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <Button
        android:id="@+id/button"
        style="@style/btnSolidRounded_4"
        android:layout_width="323dp"
        android:layout_height="74dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="15dp"
        android:background="@drawable/rectangle_bg_gray_r20"
        android:singleLine="true"
        android:text="Patient Information"
        android:textAllCaps="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearColumnabout" />

</androidx.constraintlayout.widget.ConstraintLayout>